{
  "name": "@d34dman/notification-server",
  "version": "1.0.0",
  "description": "A real-time notification server with WebSocket support",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "demo:client": "node demo/client/serve-demo-client.js",
    "demo:client:port": "node demo/client/serve-demo-client.js --port"
  },
  "keywords": [
    "websocket",
    "notifications",
    "real-time",
    "redis"
  ],
  "author": "d34dman",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.10",
    "cors": "^2.8.5",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.6.0",
    "uuid": "^11.1.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/commander": "^2.12.5",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "commander": "^13.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "nodemon": "^3.0.3",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "winston": "^3.17.0"
  }
}
